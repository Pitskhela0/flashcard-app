{
  "manifest_version": 3,
  "name": "Flashcard Helper",
  "version": "1.0",
  "description": "An extension to help create flashcards from web pages.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}